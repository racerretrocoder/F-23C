<PropertyList>
 <sim>
  <flight-model>jsb</flight-model>
  <status>Alpha v1.0a</status>
  <author>Lee Elliott, StuartC</author>
  <aero>fdm</aero>
  <fuel-fraction>1.0</fuel-fraction>

  <description>Northrop / McDonnell Douglas YF-23 Advanced Tactical Fighter</description>
  <rating>
<FDM type="int">4</FDM>
<systems type="int">1</systems>
<cockpit type="int">2</cockpit>
<model type="int">3</model>
</rating>

 
  
   <startup>
   <splash-texture>Aircraft/F-23C/splash.png</splash-texture>
  </startup>

  <view n="0">
   <internal archive="y">true</internal>
   <config>
    <x-offset-m archive="y">0.0088</x-offset-m>
    <y-offset-m archive="y">1.0453125</y-offset-m>
    <z-offset-m archive="y">-5.84296875</z-offset-m>
    <pitch-offset-deg>-8</pitch-offset-deg>
    <default-field-of-view-deg>89</default-field-of-view-deg>
   </config>
  </view>

  <current-view>
   <field-of-view>89</field-of-view>
  </current-view>
  
  <view n="100">
	   <name>Pilot cam</name>
        <internal archive="y">false</internal>
        <config>
		 <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <x-offset-m archive="y">0.00</x-offset-m>
    <y-offset-m archive="y">1.10</y-offset-m>
    <z-offset-m archive="y">-6.1</z-offset-m>
          <pitch-offset-deg>0</pitch-offset-deg>
		  <heading-offset-deg>180</heading-offset-deg>
          <default-field-of-view-deg>95</default-field-of-view-deg>
        </config>
      </view>

  <sound>
   <path>Aircraft/F-23C/Sounds/sound.xml</path>
  </sound>

  

    <hud>
   <path n="1">Aircraft/F-23C/Hud/hud.xml</path>

   <visibility n="1">true</visibility>
   <palette>
    <color n="0">
     <alpha type="float">0.85</alpha>
     <antialiased type="bool">true</antialiased>
     <brightness type="float">0.85</brightness>
     <transparent type="bool">true</transparent>
     <red type="float">0.38</red>
     <green type="float">1.0</green>
     <blue type="float">0.22</blue>
    </color>
    <color n="1">
     <alpha type="float">0.85</alpha>
     <antialiased type="bool">true</antialiased>
     <brightness type="float">0.85</brightness>
     <transparent type="bool">true</transparent>
     <red type="float">1.0</red>
     <green type="float">0.2</green>
     <blue type="float">0.0</blue>
    </color>
   </palette>
   <clipping>
            <left type="double">-195</left>
            <right type="double">195</right>
            <top type="double">65</top>
            <bottom type="double">-170</bottom>
         </clipping>
  </hud>


  <model>
   <path>Aircraft/F-23C/Models/YF-23.xml</path>
  </model>

  <submodels> 
    		<serviceable type="bool">true</serviceable>
            <path>Aircraft/F-23C/Submodels.xml</path>
  		</submodels>
  
  <systems>
    <autopilot>
        <path>Aircraft/F-23C/Systems/test.xml</path>
      </autopilot>
   <electrical>
    <path>Aircraft/Generic/generic-electrical.xml</path>
   </electrical>
   <refuel>
<type>boom</type>
<serviceable type="bool">true</serviceable>
</refuel>
  </systems>
  

		<rendering>
			<redout>
				<parameters>
					<blackout-complete-g>15</blackout-complete-g>
					<blackout-onset-g>15</blackout-onset-g>
				</parameters>
			</redout>
		</rendering>
	

<mp-systems>
<radar-enabled type="bool">true</radar-enabled>
<ecm-rwr-enabled type="bool">true</ecm-rwr-enabled>
<combat-enabled type="bool">true</combat-enabled>
</mp-systems>	


<multiplay n="0">
      <chat_display>1</chat_display>
      <generic n="0">
	
       
	 <float n="31" type="double" alias="/fdm/jsbsim/fcs/ab1"/>	
 <float n="32" type="double" alias="/fdm/jsbsim/fcs/ab2"/>	  <!--Burner-->

	
      
      </generic>
    </multiplay>	

  <chase-distance-m type="double" archive="y">-30.0</chase-distance-m>

<!-- Flap settings -->
  <flaps>
   <setting>0.00</setting>
   <setting>0.08</setting>
   <setting>0.16</setting>
   <setting>0.32</setting>
   <setting>0.48</setting>
   <setting>0.64</setting>
   <setting>0.82</setting>
   <setting>1.00</setting>
  </flaps>

  <help>
   <title>F-23C</title>
   <key>
    <name>}</name>
    <desc>Start / Stop engines</desc>
   </key>
   <key>
        <name>d</name>
        <desc>Open/Close canopy</desc>
      </key>
	  <key>
        <name>q</name>
        <desc>Radar Stadby mode</desc>
      </key>
	   <key>
        <name>T</name>
        <desc>Toggle RWR</desc>
      </key>
      <key>
        <name>t</name>
        <desc>Change Radar Distances (10, 20 50,100,150)</desc>
      </key>
      <key>
        <name>R</name>
        <desc>Activate/desactivate targeting the nearest (the target have to be bellow radar range)</desc>
      </key>
      <key>
        <name>H</name>
        <desc>Toggle Armements Circles</desc>
      </key>
	   <key>
        <name>n</name>
        <desc>Next Target</desc>
      </key>
      <key>
        <name>Ctrl-n</name>
        <desc>Previous Target</desc>
      </key>  
	  
   <key>
    <name>e</name>
    <desc>fIRE Cannon</desc>
   </key>
   
  </help>
  
  
