<?xml version="1.0"?>

<PropertyList>

    <path>ELEC.ac</path>


 
  <animation>
         <object-name>base</object-name>
     <type>material</type>
     <emission> 
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        <red>0.1</red>
        <green>0.8</green>
        <blue>0.1</blue>
      </emission>
  </animation> 



  <animation>
  <object-name>face</object-name>
   <type>material</type>
     <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        <red>  0.5 </red>
        <green>0.5 </green>
        <blue> 0.4 </blue>
     </emission>
  </animation> 







<animation>
        <type>knob</type>
    <object-name>gen1</object-name>
        <value>0.0</value> <!-- or property as usual for rotate animation -->        
        <action>
            <binding>
                <command>property-cycle</command>
                <property>/controls/electric/gen1</property>
                <value>0</value>
                <value>1</value>           
                <wrap>false</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>gen1</tooltip-id>
                <label>Generator 1</label>
            </binding>
        </hovered>
  <increase>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/clicksmall</property>
      </binding>
        </increase>
        <decrease>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/clicksmall</property>
      </binding>
        </decrease> 
    </animation>




 <animation>
  <object-name>gen1</object-name>
  <type>rotate</type>
  <property>/controls/electric/gen1</property>
  <factor>25</factor>
  <offset-deg>0</offset-deg>
  <axis>
  <object-name>line</object-name>
  </axis>
 </animation>






