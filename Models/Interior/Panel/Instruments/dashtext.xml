  <animation>
        <type>select</type>
        <object-name>ap</object-name>
        
        <condition>
 <or>
  <not-equals>
   <property>/autopilot/locks/heading</property>
   <value></value>
  </not-equals>
  <not-equals>
   <property>/autopilot/locks/altitude</property>
   <value></value>
  </not-equals>
  <not-equals>
   <property>/autopilot/locks/speed</property>
   <value></value>
  </not-equals>
  <and>
  <not-equals>
   <property>/autopilot/locks/heading</property>
   <value></value>
  </not-equals>
  <not-equals>
   <property>/autopilot/locks/altitude</property>
   <value></value>
  </not-equals>
  </and>
  <and>
  <not-equals>
   <property>/autopilot/locks/altitude</property>
   <value></value>
  </not-equals>
  <not-equals>
   <property>/autopilot/locks/speed</property>
   <value></value>
  </not-equals>
  </and>
  <and>
  <not-equals>
   <property>/autopilot/locks/heading</property>
   <value></value>
  </not-equals>
  <not-equals>
   <property>/autopilot/locks/speed</property>
   <value></value>
  </not-equals>
  </and>
 </or>
</condition>
    </animation>
 
 
 
 
 
 
 
