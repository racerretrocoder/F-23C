<?xml version="1.0"?>

<PropertyList>

    <path>audio.ac</path>
  
 
  <animation>
 <object-name>base</object-name>
   <type>material</type>
     <emission> 
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        <red>0.1</red>
        <green>0.8</green>
        <blue>0.1</blue>
      </emission>
  </animation>


 
  <animation>
        <type>knob</type>
    <object-name>vft</object-name>
        <value>0.0</value> <!-- or property as usual for rotate animation -->        
        <action>
            <binding>
                <command>property-cycle</command>
                <property>/controls/vms/vft</property>
                <value>0</value>
                <value>1</value>           
                <wrap>false</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>gen1</tooltip-id>
                <label>This switch is not used. (Non Functional on the real YF-23)</label>
            </binding>
        </hovered>
  <increase>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/clicksmall</property>
      </binding>
        </increase>
        <decrease>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/clicksmall</property>
      </binding>
        </decrease> 
    </animation>




 <animation>
  <object-name>vft</object-name>
  <type>rotate</type>
  <property>/controls/vms/vft</property>
  <factor>45</factor>
  <offset-deg>0</offset-deg>
  <axis>
  <object-name>line2</object-name>
  </axis>
 </animation>



