<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

<animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>500</max-m>
 </animation>

	<effect>
      <inherits-from>Effects/model-transparent</inherits-from>
         <object-name>0fdd30ec_dds</object-name>
   </effect>
  


  
  <!-- pilots   -->
<model>
<name>pilot</name>
    <path>Aircraft/F-23C/Models/Interior/pilots/pilot.xml</path>
    <offsets>
      <x-m> -5.05</x-m>
      <y-m> -0.02 </y-m>
      <z-m> 0.9 </z-m>
    </offsets>
  </model>
  
  

  
  
  <!-- Pilot invisible from internally -->

    <animation>
        <type>select</type>
        <object-name>pilot</object-name>
		 <object-name>wso</object-name>
		<condition>
<not>
            <property>sim/current-view/internal</property>
</not>
        </condition>
    </animation>		
  
  
  <!-- Dash-->
 
<model>
    <path>cockpit.ac</path>
    <offsets>
      <x-m> 0 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.00 </z-m>
    </offsets>
  </model>

  <!-- HUD Diamond -->
  
  
  <model>
  
    <name>HUD_ADDS</name>
      <path>Aircraft/Instruments-3d/radar2/radar-screen.xml</path>
    <name>diamond</name>
    <offsets>
      <x-m>-4.76</x-m>
   <y-m>-0.06</y-m>
   <z-m>0.95</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
  </model>
  
<!-- instruments -->

<model>
    <path>Aircraft/F-23C//Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m>-0.895</x-m>
      <y-m> 0.068 </y-m>
      <z-m>0.4 </z-m>
    </offsets>
  </model>

